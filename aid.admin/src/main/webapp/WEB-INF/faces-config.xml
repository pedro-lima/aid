<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">

	<application>
		<resource-bundle>
			<base-name>messages</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>

	<navigation-rule>
		<from-view-id>/homologacao/software/listar.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>cadastrar_software</from-outcome>
			<to-view-id>/homologacao/software/cadastrar.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
